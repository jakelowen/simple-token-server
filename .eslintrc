{
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": 2,
    "no-undef": 2
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint"
}
